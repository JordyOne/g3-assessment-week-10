<h1>New gif</h1>


<%= form_for @gifs, url: gif_index_path do |f| %>
    <div>
      <%= f.label :image_url %>
      <%= f.text_field :image_url %>
    </div>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.submit "Create gif" %>
    </div>
<% end %>